<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.uestc.museum.job.dao.MinuteFlowMapper">
    <select id="queryByMonth" resultType="cn.edu.uestc.museum.job.entity.MinuteFlow">
        select * from mus_miniute_flow where DATE_FORMAT(collect_time,'%Y-%m-%d')= DATE_FORMAT(#{time},'%Y-%m-%d')
    </select>

</mapper>